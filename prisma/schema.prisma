generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@127.0.0.1:3306/landing"
}

model User{
  id Int @id @default(autoincrement())
  title String
  name String
  body String
  phone String
}